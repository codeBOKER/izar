# Example environment variables for the project
VITE_API_URL=YOUR_VITE_API_URL

# Supabase client (used by the frontend)
VITE_SUPABASE_URL=YOUR_SUPABASE_URL
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Supabase Postgres connection (used by schema export script)
# Find this in Supabase Dashboard → Project Settings → Database → Connection string
# Format: postgresql://postgres:PASSWORD@db.PROJECT_REF.supabase.co:5432/postgres
SUPABASE_DB_URL=postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres
